let black_tile = [ [ 1 ] ]
let white_tile = [ [ 0 ] ]

let result_tile = []

for i in 0..63 {
    let row = []

    for j in 0..63 {
        if (i + j) % 2 == 0 {
            row = row ++ black_tile
        }
        else {
            row = row ++ white_tile
        }
    }

    result_tile = result_tile :: row
}

output result_tile

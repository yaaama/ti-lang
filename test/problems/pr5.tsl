import "tile1" as tile1

let subtile1 = []
let subtile2 = []
let subtile3 = []
let result_tile = []

subtile1 = take 0 0 6 tile1
subtile2 = take 2 2 6 tile1
subtile3 = take 4 4 6 tile1

let row1 = []
let row2 = []
let row3 = []

for i in 0..2 {
    row1 = row1 ++ subtile1
    row2 = row2 ++ subtile2
    row3 = row3 ++ subtile3
}

result_tile = result_tile :: row1
result_tile = result_tile :: row2
result_tile = result_tile :: row3

output result_tile

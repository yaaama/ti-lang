import "tile1" as tile1
import "tile2" as tile2
import "tile3" as tile3

let blank = []
let base_tile = []
let result_tile = []

for i in 0..49 {
    let row = []
    let temp = []

    for j in 0..49 {
        if ((i + j) < 50) && (j < 25) {
            temp = ? tile3
            temp = tile1 & temp
            row = row ++ temp
        }
        else {
            if (i <= j) && (j >= 25) {
                temp = ? tile3
                temp = tile2 & temp
                row = row ++ temp
            }
            else {
                row = row ++ blank
            }
        }
    }

    result_tile = result_tile :: row
}

output result_tile
